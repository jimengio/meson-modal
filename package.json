{
  "name": "ts-workflow",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --hot --config webpack/dev.js",
    "watch": "webpack-dev-server --hot --config webpack/dev.js",
    "dll": "rm -rfv webpack/dll/* && time webpack --config webpack/dll-dev.js",
    "dll-release": "time webpack --config webpack/dll-release.js",
    "release": "rm -rfv dist/* && yarn dll-release && time webpack --config webpack/release.js",
    "build": "yarn release",
    "serve": "http-server dist/ -s",
    "compile": "tsc -d --project tsconfig-compile.json --outDir lib/",
    "upload": "rsync -avr --progress dist/ fe.jimu.io:~/repo/jimengio/ts-workflow",
    "gen-router": "ts-node -P tsconfig-node.json -r tsconfig-paths/register example/gen-router.ts",
    "postinstall": "rm -rfv node_modules/@types/*/node_modules/@types/react"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jimengio/ci-progress-webpack-plugin": "^0.1.0",
    "@jimengio/router-code-generator": "^0.1.5",
    "@types/lodash": "^4.14.138",
    "@types/node": "^12.7.3",
    "@types/prettier": "^1.18.2",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "cache-loader": "^4.1.0",
    "css-loader": "^3.2.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "file-loader": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-tags-plugin": "^2.0.15",
    "http-server": "^0.11.0",
    "mini-css-extract-plugin": "^0.8.0",
    "prettier": "^1.18.2",
    "style-loader": "^1.0.0",
    "thread-loader": "^2.1.3",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.6.2",
    "typescript-styled-plugin": "^0.14.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-hud": "^0.1.2"
  },
  "dependencies": {
    "@jimengio/ruled-router": "^0.2.15",
    "dayjs": "^1.8.16",
    "emotion": "^10.0.17",
    "font-awesome": "^4.7.0",
    "immer": "^3.2.0",
    "lodash": "^4.17.15",
    "query-string": "^6.8.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
