{
  "name": "@jimengio/meson-modal",
  "version": "0.3.9",
  "description": "Modal component for Meson Form and other usages",
  "main": "lib/index.js",
  "scripts": {
    "dev": "webpack-dev-server --hot --config webpack/dev.js",
    "watch": "webpack-dev-server --hot --config webpack/dev.js",
    "dll": "rm -rfv webpack/dll/* && time webpack --config webpack/dll-dev.js",
    "dll-release": "time webpack --config webpack/dll-release.js",
    "release": "rm -rfv dist/* && yarn dll-release && time webpack --config webpack/release.js",
    "build": "yarn release",
    "compile": "rm -rfv lib/* && tsc -d --project tsconfig-compile.json --outDir lib/",
    "upload": "rsync -avr --progress dist/ fe.jimu.io:~/repo/jimengio/meson-modal",
    "gen-router": "ts-node -P tsconfig-node.json -r tsconfig-paths/register example/gen-router.ts",
    "postinstall": "rm -rfv node_modules/@types/*/node_modules/@types/react"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@jimengio/ci-progress-webpack-plugin": "^0.1.0",
    "@jimengio/doc-frame": "^0.1.12",
    "@jimengio/flex-styles": "^0.1.6",
    "@jimengio/jimo-basics": "^0.1.7",
    "@jimengio/jimo-icons": "^0.3.11",
    "@jimengio/router-code-generator": "^0.2.7",
    "@jimengio/ruled-router": "^0.2.26",
    "@types/node": "^14.0.14",
    "@types/prettier": "^2.0.1",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "cache-loader": "^4.1.0",
    "css-loader": "^3.6.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "file-loader": "^6.0.0",
    "font-awesome": "^4.7.0",
    "fork-ts-checker-webpack-plugin": "^5.0.5",
    "html-webpack-plugin": "^4.3.0",
    "html-webpack-tags-plugin": "^2.0.17",
    "immer": "^7.0.5",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "style-loader": "^1.2.1",
    "thread-loader": "^2.1.3",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.5",
    "typescript-styled-plugin": "^0.15.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-hud": "^0.1.2"
  },
  "peerDependencies": {
    "@jimengio/flex-styles": "*",
    "@jimengio/jimo-icons": "*",
    "font-awesome": "^4.7.0",
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "dayjs": "^1.8.28",
    "emotion": "^10.0.27",
    "query-string": "^6.13.1",
    "react-transition-group": "^4.4.1"
  }
}
