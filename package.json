{
  "name": "@jimengio/meson-modal",
  "version": "0.2.23",
  "description": "Modal component for Meson Form and other usages",
  "main": "lib/index.js",
  "scripts": {
    "dev": "webpack-dev-server --hot --config webpack/dev.js",
    "watch": "webpack-dev-server --hot --config webpack/dev.js",
    "dll": "rm -rfv webpack/dll/* && time webpack --config webpack/dll-dev.js",
    "dll-release": "time webpack --config webpack/dll-release.js",
    "release": "rm -rfv dist/* && yarn dll-release && time webpack --config webpack/release.js",
    "build": "yarn release",
    "serve": "http-server dist/ -s",
    "compile": "rm -rfv lib/* && tsc -d --project tsconfig-compile.json --outDir lib/",
    "prepare": "yarn compile",
    "upload": "rsync -avr --progress dist/ fe.jimu.io:~/repo/jimengio/meson-modal",
    "gen-router": "ts-node -P tsconfig-node.json -r tsconfig-paths/register example/gen-router.ts",
    "postinstall": "rm -rfv node_modules/@types/*/node_modules/@types/react"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@jimengio/ci-progress-webpack-plugin": "^0.1.0",
    "@jimengio/doc-frame": "^0.1.8",
    "@jimengio/flex-styles": "^0.1.6",
    "@jimengio/jimo-basics": "^0.0.1",
    "@jimengio/jimo-icons": "^0.3.0",
    "@jimengio/router-code-generator": "^0.2.5",
    "@jimengio/ruled-router": "^0.2.21",
    "@jimengio/shared-utils": "^0.1.5",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.12",
    "@types/prettier": "^1.19.0",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "cache-loader": "^4.1.0",
    "css-loader": "^3.2.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "file-loader": "^4.3.0",
    "font-awesome": "^4.7.0",
    "fork-ts-checker-webpack-plugin": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-tags-plugin": "^2.0.17",
    "http-server": "^0.11.0",
    "mini-css-extract-plugin": "^0.8.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "style-loader": "^1.0.0",
    "thread-loader": "^2.1.3",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.2",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.2",
    "typescript-styled-plugin": "^0.15.0",
    "url-loader": "^2.3.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-hud": "^0.1.2"
  },
  "peerDependencies": {
    "@jimengio/flex-styles": "*",
    "@jimengio/jimo-icons": "*",
    "font-awesome": "^4.7.0",
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "dayjs": "^1.8.17",
    "emotion": "^10.0.23",
    "immer": "^5.0.0",
    "lodash": "^4.17.15",
    "query-string": "^6.9.0",
    "react-transition-group": "^4.3.0",
    "use-immer": "^0.3.5"
  }
}
